<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Circular Progress Bar | CodingNepal</title>
    <link rel="stylesheet" href="/css/dashboard_style.css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-circle-progress/1.2.2/circle-progress.min.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
  </head>
  <body>
    <nav class="navbar">
      <div class="navbar__container">
          <div class="navbar__toggle" id="mobile-menu">
              <span class="bar"></span>
              <span class="bar"></span>
              <span class="bar"></span>
          </div>
          <ul class="navbar__menu">
              <li class="navbar__item">
                  <a href="/dashboard" class="navbar__links" id="home-page">Dashboard</a>
              </li>
              <li class="navbar__item">
                  <a href="/dashboard/facility" class="navbar__links" id="about-page">Facility</a>
              </li>
              <li class="navbar__item">
                <a href="/dashboard/patients" class="navbar__links" id="about-page">Patients</a>
            </li>
              
          </ul>
      </div>
  </nav>
        <div class="wrapper">
          
          
          <div class="card">
            <div class="text">Ordinary Bed</div>

            <div class="circle" >

              <div class="bar"></div>

              <div class="box">
                <span></span>
              </div>

            </div>
            
            <div class="text padd"><%=(beds_data[0].total - beds_data[0].occupied)%>/<%=beds_data[0].total%></div>
           
          </div>

          <div class="card one">
            <div class="text">Oxygen Beds</div>
            <div class="circle">
              <div class="bar"></div>
              <div class="box"><span></span></div>
            </div>
            
            <div class="text padd"><%=(beds_data[1].total - beds_data[1].occupied)%>/<%=beds_data[1].total%></div>
          </div>
          <div class="card two">
            <div class="text">ICU</div>
            <div class="circle">
              <div class="bar"></div>
              <div class="box"><span></span></div>
            </div>
            
            <div class="text padd"><%=(beds_data[2].total - beds_data[2].occupied)%>/<%=beds_data[2].total%></div>
          </div>
        
          <div class="card three">
            <div class="text">Ventilator</div>
            <div class="circle">
              <div class="bar"></div>
              <div class="box"><span></span></div>
            </div>
            
            <div class="text padd"><%=(beds_data[3].total - beds_data[3].occupied)%>/<%=beds_data[3].total%></div>
            </div>
            
              <div class="card four">
                <div class="text"> <span style="color: red;"> Covid </span> Ordinary Bed</div>
                <div class="circle">
                  <div class="bar"></div>
                  <div class="box"><span></span></div>
                  
                </div>
                
                <div class="text padd"><%=(beds_data[4].total - beds_data[4].occupied)%>/<%=beds_data[4].total%></div>
              </div>
              <div class="card five">
                <div class="text"> <span style="color: red;"> Covid </span> Oxygen Beds</div>
                <div class="circle">
                  <div class="bar"></div>
                  <div class="box"><span></span></div>
                  
                </div>
                
                <div class="text padd"><%=(beds_data[5].total - beds_data[5].occupied)%>/<%=beds_data[5].total%></div>
              </div>
              <div class="card six">
                <div class="text"> <span style="color: red;"> Covid </span> ICU</div>
                <div class="circle">
                  <div class="bar"></div>
                  <div class="box"><span></span></div>
                 
                </div>
                
                <div class="text padd"><%=(beds_data[6].total - beds_data[6].occupied)%>/<%=beds_data[6].total%></div>
              </div>
              <div class="card seven">
                <div class="text"><span style="color: red;"> Covid </span>  ICU w/ Ventilator</div>
                <div class="circle">
                  <div class="bar"></div>
                  <div class="box"><span></span></div>
                </div>
                
                <div class="text padd"><%=(beds_data[7].total - beds_data[7].occupied)%>/<%=beds_data[7].total%></div>
              </div>
          
          </div>
        
   
      
    
<script>

function get_color(value){
    if(value < 0.50 ){
        color = '#008000'
    }else if( value < 0.75){
        color = '#FFA500'
    }else{
        color = '#FF0000'
    }
    return color
}

let value = 0;

let options = {
  startAngle: -1.55,
  size: 150,
  value: <%=beds_data[0].perc%>,
  fill : '<%=beds_data[0].colorcode%>'
}

$(".circle .bar").circleProgress(options).on('circle-animation-progress',
  function(event, progress, stepValue){
    $(this).parent().find("span").text(String(stepValue.toFixed(2).substr(2)) + "%");
});

$(".one .bar").circleProgress({
  value: <%=beds_data[1].perc%>,
  fill : '<%=beds_data[1].colorcode%>'
});

$(".two .bar").circleProgress({
  value: <%=beds_data[2].perc%>,
  fill : '<%=beds_data[2].colorcode%>'
});

$(".three .bar").circleProgress({
  value: <%=beds_data[3].perc%>,
  fill : '<%=beds_data[3].colorcode%>'
});

$(".four .bar").circleProgress({
  value: <%=beds_data[4].perc%>,
  fill : '<%=beds_data[4].colorcode%>'
});

$(".five .bar").circleProgress({
  value: <%=beds_data[5].perc%>,
  fill : '<%=beds_data[5].colorcode%>'
});

$(".six .bar").circleProgress({
  value: <%=beds_data[6].perc%>,
  fill : '<%=beds_data[6].colorcode%>'
});

$(".seven .bar").circleProgress({
  value: <%=beds_data[7].perc%>,
  fill : '<%=beds_data[7].colorcode%>'
});

</script>

<script src="/js/app.js"></script>
 
  </body>
  
</html>